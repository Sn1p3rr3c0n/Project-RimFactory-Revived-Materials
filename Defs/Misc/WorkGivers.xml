<?xml version="1.0" encoding="UTF-8"?>

<Defs>

  <WorkGiverDef>
    <defName>DigAtDiggingSpot</defName>
    <label>dig at digging spot</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Mining</workType>
    <priorityInType>20</priorityInType>
    <fixedBillGiverDefs>
      <li>PRF_DigSpot</li>
    </fixedBillGiverDefs>
        <verb>dig</verb>
    <gerund>digging</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>

  <WorkGiverDef>
    <defName>GrindingBench</defName>
    <label>grind materials at grindstone</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Crafting</workType>
    <priorityInType>20</priorityInType>
    <fixedBillGiverDefs>
      <li>PRF_GrindStone</li>
    </fixedBillGiverDefs>
        <verb>grind</verb>
    <gerund>grinding</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>
  
</Defs>

