	<!-- ================== Royalty patches =====================-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- Set favor value for all Gemstones -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="PRF_RawDiamond"]/statBases</xpath>
					<value>
						<RoyalFavorValue>0.035</RoyalFavorValue>
					</value>
				</li>

				<!-- If Jewelry mod isnt installed add... -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Jewelry</li>
					</mods>
					<nomatch Class="PatchOperationSequence">
						<operations>
							<!-- Update trader to accept Gemstones trade tag -->
							<li Class="PatchOperationAdd">
								<xpath>/Defs/TraderKindDef[defName="Empire_Caravan_TributeCollector"]/stockGenerators</xpath>
								<value>
									<li Class="StockGenerator_BuyTradeTag">
										<tag>Gemstones</tag>
									</li>
								</value>
							</li>
						</operations>
					</nomatch>
				</li>
			</operations>
		</match>
	</Operation>

